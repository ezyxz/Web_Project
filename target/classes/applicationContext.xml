<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="myBean" class="com.test.MyImp" scope="prototype" init-method="init" destroy-method="destory"></bean>
<!--    id： Bean实例在Spring 容器中的唯一标识-->
<!--    class： Bean的全限定名-->
<!--    scope：对象作用范围-->
<!--   singleton 在生成容器时 对象生成 和 prototype- getBean时 对象生成 -->
    <bean id="myConstructor" class="com.constructor.Factory"   factory-method="staticGetObj"></bean>
    <bean id="myFactory" class="com.constructor.Factory"></bean>
    <bean id="myDynamicConstructor" factory-bean="myFactory"  factory-method="dynamicGetObj"></bean>
<!--    依赖注入-->
    <bean id="getStudent" class="com.test.Student" ></bean>
   <bean id="getTeacherWithDep" class="com.test.Teacher" >
       <property name="student" ref="getStudent"></property>
   </bean>
<!--    构造方法-->
    <bean id="getTeacherWithParm" class="com.test.Teacher" >
        <constructor-arg name="student" ref="getStudent"></constructor-arg>
        <property name="name" value="WONG"></property>
        <property name="map">
            <map>
                <entry key="1" value-ref="getStudent"></entry>
            </map>
        </property>
    </bean>

<import resource="applicationContext_jdbc.xml"></import>

</beans>